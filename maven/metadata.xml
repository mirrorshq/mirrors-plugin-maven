<?xml version="1.0" encoding="utf-8"?>
<mirror-site id="maven">
  <name>Maven Central Repository Mirror</name>
  <storage type="file"/>
  <advertiser type="httpdir"/>
  <initializer>
    <executable>maven_initializer.py</executable>
  </initializer>
  <updater>
    <executable>maven_updater.py</executable>
    <cron-expression>0 * * * *</cron-expression>
  </updater>
</mirror-site>
